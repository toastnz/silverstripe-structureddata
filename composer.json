{
    "name": "toastnz/silverstripe-structureddata",
    "description": "using code from michelsteege/silverstripe-structureddata as module has vanished",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "structured",
        "data",
        "json-ld"
    ],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Michel van der Steege",
        "email": "michelsteege@hotmail.com"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "silverstripe/cms": "^4.0@dev",
        "silverstripe/vendor-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "installer-name": "silverstripe-structureddata",
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Pixelspin\\StructuredData\\": "src/"
        }
    }
}
